{
    "name": "mrpunyapal/laravel-extended-commands",
    "description": "This package will have some commands which are not available in laravel.",
    "keywords": [
        "Punyapal Shah",
        "laravel",
        "laravel-extended-commands",
        "make Builder",
        "make commands",
        "dev"
    ],
    "homepage": "https://github.com/mrpunyapal/laravel-extended-commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Punyapal Shah",
            "email": "mrpunyapal@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3|^8.4|^8.5",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9||^3.0",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0||^4.0",
        "pestphp/pest-plugin-arch": "^3.0||^4.0",
        "pestphp/pest-plugin-laravel": "^3.0||^4.0",
        "phpstan/extension-installer": "^1.3||^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.1||^2.0",
        "phpstan/phpstan-phpunit": "^1.3||^2.0",
        "rector/rector": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "MrPunyapal\\LaravelExtendedCommands\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MrPunyapal\\LaravelExtendedCommands\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "lint":[
            "rector",
            "pint --parallel"
        ],
        "test:lint": [
            "rector --dry-run",
            "pint --test --parallel"
        ],
        "test:types": "phpstan analyse",
        "test:arch": "pest --filter=arch",
        "test:unit": "pest --parallel",
        "test": [
            "@test:lint",
            "@test:types",
            "@test:type-coverage",
            "@test:unit"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrPunyapal\\LaravelExtendedCommands\\LaravelExtendedCommandsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
